  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vaibhav Pandey",
      "email": "contact@vaibhavpandey.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "vaibhavpandeyvpz/databoss": "^2.1"
  },
  "require-dev": {
